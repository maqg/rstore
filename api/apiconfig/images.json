{
   	"APIAddImage": {
		"name": "添加镜像",
		"paras": [
			{
				"name": "name",
				"type": "string",
				"desc": "镜像名称",
				"default": "NotNull"
			},
			{
				"name": "id",
				"type": "string",
				"desc": "镜像ID",
				"default": "NotNull"
			},
			{
				"name": "accountId",
				"type": "string",
				"desc": "Account ID",
				"default": "NotNull"
			},
			{
				"name": "accountId",
				"type": "string",
				"desc": "Account ID",
				"default": "NotNull"
			},
			{
				"name": "url1",
				"type": "string",
				"desc": "镜像来源的URL地址1",
				"default": "NotNull"
			},
			{
				"name": "url2",
				"type": "string",
				"desc": "镜像来源的URL地址2",
				"default": ""
			},
			{
				"name": "url3",
				"type": "string",
				"desc": "镜像来源的URL地址3",
				"default": ""
			},
			{
				"name": "format",
				"type": "string",
				"desc": "镜像格式。raw、qcow2、iso",
				"default": "qcow2"
			},
			{
				"name": "arch",
				"type": "string",
				"desc": "amd64 or x86",
				"default": "amd64"
			},
			{
				"name": "mediaType",
				"type": "string",
				"desc": "镜像的媒体类型，RootVolumeTemplate，DataValumeTemplate, ISO",
				"default": "RootVolumeTemplate"
			},
			{
				"name": "platform",
				"type": "string",
				"desc": "平台类型，如Linux，Windows， Other",
				"default": "Linux"
			},
			{
				"name": "guestOsType",
				"type": "string",
				"desc": "客户操作系统类型，如CentOS，Debian",
				"default": "Debian7"
			},
			{
				"name": "isSystem",
				"type": "bool",
				"desc": "是否为系统模板",
				"default": false
			},
			{
				"name": "username",
				"type": "string",
				"desc": "FTP用户名",
				"default": ""
			},
			{
				"name": "password",
				"type": "string",
				"desc": "FTP密码",
				"default": ""
			}
		]
	},

	"APIShowImage": {
		"name": "获取单个镜像",
		"paras": [
			{
				"name": "id",
				"type": "string",
				"desc": "账号的UUID",
				"default": "NotNull"
			}
		]
	},

	"APIShowAllImages": {
		"name": "获取所有镜像",
		"paras": [
			{
				"name": "start",
				"type": "int",
				"desc": "开始位置",
				"default": 0
			},
			{
				"name": "limit",
				"type": "int",
				"desc": "获取条目",
				"default": 15
			},
			{
				"name": "accountId",
				"type": "string",
				"desc": "账号ID",
				"default": ""
			},
			{
				"name": "mediaType",
				"type": "string",
				"desc": "镜像类型，RootVolumeTemplate，DataValumeTemplate, ISO",
				"default": ""
			},
			{
				"name": "keyword",
				"type": "string",
				"desc": "Key word for image name or Id",
				"default": ""
			}
		]
	},

	"APIShowAccountList": {
		"name": "查看账号列表",
		"handler": "ShowAccountList",
		"paras": []
	},

	"APIDeleteImage": {
		"name": "删除镜像",
		"handler": "DeleteImage",
		"paras": [
			{
				"name": "id",
				"type": "string",
				"desc": "账号的UUID",
				"default": "NotNull"
			},
			{
				"name": "mediaType",
				"type": "string",
				"desc": "镜像类型，RootVolumeTemplate，DataValumeTemplate, ISO",
				"default": "RootVolumeTemplate"
			},
			{
				"name": "accountId",
				"type": "string",
				"desc": "账号ID",
				"default": ""
			}
		]
	},

	"APIUpdateImage": {
		"name": "更新镜像",
		"paras": [
			{
				"name": "id",
				"type": "string",
				"desc": "镜像Id",
				"default": "NotNull"
			},
			{
				"name": "name",
				"type": "string",
				"desc": "镜像名称",
				"default": ""
			}
		]
	}
}
